<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_analysis">

    <fragment
        android:id="@+id/navigation_analysis"
        android:name="com.dasktelor.myapplication.ui.analysis.AnalysisFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_analysis" />

    <fragment
        android:id="@+id/navigation_results"
        android:name="com.dasktelor.myapplication.ui.results.ResultsFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_results" />

    <fragment
        android:id="@+id/navigation_support"
        android:name="com.dasktelor.myapplication.ui.support.SupportFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.dasktelor.myapplication.ui.profile.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>